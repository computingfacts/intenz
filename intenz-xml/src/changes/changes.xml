<?xml version="1.0" encoding="UTF-8"?>

<document xmlns="http://maven.apache.org/changes/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/plugins/maven-changes-plugin/xsd/changes-1.0.0.xsd">

    <properties>
        <title>IntEnz XML module change log</title>
        <author email="rafael.alcantara@ebi.ac.uk">Rafael Alc√°ntara</author>
    </properties>
    
    <body>
		<release version="1.0.4" date="In SVN"
			description="Added ChEBI IDs for cofactors.">
    		<action dev="rafalcan" type="add">
    		    Added ChEBI IDs for cofactors, as well as the and/or operators
    		    as text.
    		</action>
		</release>
		<release version="1.0.3" date="2010-04-15"
			description="Fixed constraint for EC numbers.">
    		<action dev="rafalcan" type="fix">
    		    Fixed the constraint applied to EC numbers. Now the uniqueness
                within sub-subclass applies to the tuple ec4-preliminary.
    		</action>
		</release>
		<release version="1.0.2" date="2009-12-01"
			description="Added Rhea to databases enumeration.">
    		<action dev="rafalcan" type="add">
    			Added Rhea to the enumeration of databases used for cross references.
    		</action>
		</release>
		<release version="1.0.1" date="2009-10-09"
			description="Preliminary EC numbers enabled">
    		<action dev="rafalcan" type="add">
    			Added support for preliminary EC numbers as an XML attribute
    			of entryType - enzyme/@ec4 attribute (it was already covered
    			in the regular expression for ecNumberType - enzyme/ec element).
    		</action>
		</release>
    	<release version="1.0" date="2009-09-30"
    		description="First release">
    		<action dev="rafalcan" type="add">
    			First release as a separate module from intenz-export.
    		</action>
    	</release>

    </body>
</document>
