<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	
	<groupId>uk.ac.ebi.intenz.webapp</groupId>
	<artifactId>intenz-curator</artifactId>
	<version>4.2.3</version>
    <name>IntEnz curator webapp</name>
	<description>IntEnz curator tool - Web application</description>
	<packaging>war</packaging>

	<parent>
		<groupId>uk.ac.ebi.intenz</groupId>
		<artifactId>webapp</artifactId>
		<version>1.1</version>
	</parent>

    <distributionManagement>
        <repository>
            <id>biobabel-repo</id>
            <name>Biobabel private maven repository.</name>
            <url>${biobabel.maven.repo.deploy.url}</url>
        </repository>
    </distributionManagement>
	
	<build>
        <resources>
        	<resource>
        		<directory>src/main/resources</directory>
        		<filtering>true</filtering>
        	</resource>
            <resource>
                <directory>${intenz.config.dir}</directory>
                <includes>
                    <include>intenz-db-*.properties</include>
                    <include>chebi-db-public.properties</include>
                    <include>chebi-db-prod.properties</include>
                </includes>
            </resource>
        </resources>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<configuration>
					<dependentWarExcludes>
						WEB-INF/web.xml,
						WEB-INF/lib/antlr-2.7.*.jar,
						WEB-INF/lib/commons-lang-*.jar,
						WEB-INF/lib/xchars-*.jar,
						pages/view/reaction_minimal.jsp
					</dependentWarExcludes>
                    <overlays>
                        <overlay />
                        <overlay>
                            <groupId>uk.ac.ebi.rhea.webapp</groupId>
                            <artifactId>rhea-find-struts1</artifactId>
                            <excludes>
                                <exclude>WEB-INF/lib/commons-beanutils-1.6.1.jar</exclude>
                                <exclude>WEB-INF/lib/antlr-2.7.2.jar</exclude>
                                <exclude>WEB-INF/lib/log4j-1.2.8.jar</exclude>
                            </excludes>
                        </overlay>
                        <overlay>
                            <groupId>uk.ac.ebi.rhea.webapp</groupId>
                            <artifactId>rhea-search-struts1</artifactId>
                            <excludes>
                                <exclude>WEB-INF/lib/commons-beanutils-1.6.1.jar</exclude>
                                <exclude>WEB-INF/lib/antlr-2.7.2.jar</exclude>
                                <exclude>WEB-INF/lib/log4j-1.2.8.jar</exclude>
                            </excludes>
                        </overlay>
                        <overlay>
                            <groupId>uk.ac.ebi.rhea.webapp</groupId>
                            <artifactId>rhea-chebiIdAssigner</artifactId>
                            <excludes>
                                <exclude>WEB-INF/lib/commons-beanutils-1.6.1.jar</exclude>
                                <exclude>WEB-INF/lib/antlr-2.7.7.jar</exclude>
                                <exclude>WEB-INF/lib/log4j-1.2.8.jar</exclude>
                            </excludes>
                        </overlay>
                        <overlay>
                            <groupId>uk.ac.ebi.rhea.webapp</groupId>
                            <artifactId>rhea-taglib</artifactId>
                            <excludes>
                                <exclude>WEB-INF/lib/rhea-domain-*.jar</exclude>
                                <exclude>WEB-INF/lib/log4j-1.2.8.jar</exclude>
                            </excludes>
                        </overlay>
                    </overlays>
				</configuration>
			</plugin>
            <plugin>
                <groupId>org.appfuse</groupId>
                <artifactId>maven-warpath-plugin</artifactId>
                <version>2.0</version>
                <extensions>true</extensions>
                <executions>
                    <execution>
                        <goals>
                            <goal>add-classes</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
		</plugins>
	</build>

