<?xml version="1.0" encoding="UTF-8"?>
<document>
    <properties>
        <title>Change log</title>
        <author email="rafael.alcantara@ebi.ac.uk">Rafael Alc√°ntara</author>
    </properties>
    <body>
        <release version="1.0.11" date="2013-06-19"
            description="Removed parentheses">
            <action dev="rafalcan" type="remove">
                Removed rule to add parentheses for 'Si-'.
            </action>
        </release>
        <release version="1.0.10" date="2013-03-19" description="Fixed casing.">
            <action dev="rafalcan" type="update">
                Updated to latest version of parent.
            </action>
            <action dev="rafalcan" type="fix">
                Added capilalisation exception for dITP.
            </action>
            <action dev="rafalcan" type="update">
                Updated version of junit.
            </action>
        </release>
        <release version="1.0.9" date="2012-10-17"
            description="Overloaded method to bypass grammar rules">
            <action dev="rafalcan" type="add">
            	Overloaded toASCII method to be able to bypass grammar rules,
            	which are capilalising compound names in ChEBI.
            </action>
            <action dev="rafalcan" type="update">
                Updated to latest log4j version.
            </action>
        </release>
        <release version="1.0.8" date="2011-11-17"
            description="Fixed new NC-IUBMB terms for localisation">
            <action dev="rafalcan" type="fix">
            	Added pattern to avoid double parentheses around "side 1"/
            	"side 2" (the new term used by NC-IUBMB to replace cis/trans
            	and in/out).
            </action>
        </release>
        <release version="1.0.7" date="2011-06-22"
            description="Fixed aminoacid patterns">
            <action dev="rafalcan" type="fix">
            	Fixed patterns which turn aminoacid codes into lower case
            	so that they don't apply in certain cases.
            </action>
        </release>
        <release version="1.0.6" date="2011-01-26"
            description="Dependencies updates">
            <action dev="rafalcan" type="update">
                Updated to latest intenz (parent), xchars and log4j versions.
            </action>
        </release>
        <release version="1.0.5" date="2009-05-22"
            description="Un-fixed reaction rule">
            <action dev="rafalcan" type="fix">
            	Removed rule (4.1.1.39) to show protons again.
                The pre-v1.0.4 rule did not work because the equation
                had swapped sides, so the protons were not hidden.
            </action>
        </release>

        <release version="1.0.4" date="2009-05-14" description="Fixed reaction rule">
            <action dev="rafalcan" type="fix">
            	Fixed rule to hide protons (4.1.1.39).
            </action>
        </release>

        <release version="1.0.3" date="2009-03-27" description="Fixed [3Fe-4S]">
            <action dev="rafalcan" type="fix">
            	Fixed rule not to translate [3Fe-4S] into (3Fe-4S).
            </action>
            <action dev="rafalcan" type="update">
            	Updated dependencies to xchars (core and config).
            </action>
        </release>

        <release version="1.0.2" date="2008-11-17" description="Fixed spelling correction">
            <action dev="rafalcan" type="fix">
            	Fixed spelling rule for 'viron' becoming 'virion' which affected 'environment'.
            </action>
        </release>

        <release version="1.0.1" date="2008-05-07" description="Fixed triple letters">
            <action dev="rafalcan" type="fix">
                Fixed spelling rules ('l' in GB to 'll' in US english) which resulted
                in three letters in some cases.
            </action>
        </release>
        
    </body>
</document>
